<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="289.0" prefWidth="289.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddLocationSceneController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="cboxPais" editable="true" layoutX="70.0" layoutY="113.0" onKeyReleased="#openAndFilterPaises" prefWidth="150.0" promptText="Pais" />
            <ComboBox fx:id="cboxDepto" editable="true" layoutX="70.0" layoutY="156.0" onKeyReleased="#openAndFilterDeptos" prefWidth="150.0" promptText="Departamento" />
            <ComboBox fx:id="cboxMunicipio" editable="true" layoutX="70.0" layoutY="196.0" onKeyReleased="#openAndFilterMunicipios" prefWidth="150.0" promptText="Municipio" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
